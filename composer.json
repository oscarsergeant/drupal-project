{
    "name": "sergeant/drupal-project",
    "description": "SGT Project template for Drupal 9 projects with composer",
    "type": "project",
    "version": "4.0.12",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Sergeant Agency",
            "email": "drupal@sergeant.agency",
            "homepage": "https://www.sergeant.agency/",
            "role": "Developer"
        }
    ],
    "repositories": {
        "composer/packages.drupal.org/8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "sgt_sergeant/entity_creator_api": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/entity_creator_api.git"
        },
        "sgt_sergeant/sgt_checklist": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_checklist.git"
        },
        "sgt_sergeant/sgt_core": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_core.git"
        },
        "sgt_sergeant/sgt_devel": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_devel.git"
        },
        "sgt_sergeant/sgt_field_paragraph_settings": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_field_paragraph_settings.git"
        },
        "sgt_sergeant/sgt_filter_query_api": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_filter_query_api.git"
        },
        "sgt_sergeant/sgt_handbook": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_handbook.git"
        },
        "sgt_sergeant/sgt_ignite": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_ignite.git"
        },
        "sgt_sergeant/sgt_ignite_features": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_ignite_features.git"
        },
        "sgt_sergeant/sgt_installation_profile": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_installation_profile.git"
        },
        "sgt_sergeant/sgt_drupal_project_update": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_drupal_project_update.git"
        },
        "sgt_sergeant/sgt_media_colorbox_field_formatter": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_media_colorbox_field_formatter.git"
        },
        "sgt_sergeant/sgt_media_crop": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_media_crop.git"
        },
        "sgt_sergeant/sgt_paragraphs": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_paragraphs.git"
        },
        "sgt_sergeant/sgt_tiles": {
            "type": "vcs",
            "url": "git@bitbucket.org:sgt_sergeant/sgt_tiles.git"
        },
        "gfranko/jquery.tocify.js": {
            "type": "package",
            "package": {
                "name": "gfranko/jquery.tocify.js",
                "version": "1.9.0",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "jquery.tocify/js"
                },
                "dist": {
                    "url": "https://raw.githubusercontent.com/gfranko/jquery.tocify.js/master/src/javascripts/jquery.tocify.js",
                    "type": "file"
                }
            }
        },
        "harvesthq/chosen": {
            "type": "package",
            "package": {
                "name": "harvesthq/chosen",
                "version": "1.8.7",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
                    "type": "zip"
                }
            }
        },
        "jackmoore/colorbox": {
            "type": "package",
            "package": {
                "name": "jackmoore/colorbox",
                "version": "1.6.4",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
                    "type": "zip"
                }
            }
        },
        "rsportella/popper_js": {
            "type": "package",
            "package": {
                "name": "rsportella/popper_js",
                "version": "dev-master",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "popper/js"
                },
                "dist": {
                    "url": "https://unpkg.com/popper.js@1.16.0/dist/umd/popper.min.js",
                    "type": "file"
                },
                "require": {
                    "rsportella/popper_tooltip_js": "dev-master"
                }
            }
        },
        "rsportella/popper_tooltip_js": {
            "type": "package",
            "package": {
                "name": "rsportella/popper_tooltip_js",
                "version": "dev-master",
                "type": "drupal-library",
                "extra": {
                    "installer-name": "popper_tooltip/js"
                },
                "dist": {
                    "url": "https://unpkg.com/tooltip.js@1.3.2/dist/umd/tooltip.min.js",
                    "type": "file"
                }
            }
        }
    },
    "require": {
        "php": ">=7.3",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.8 || ^9",
        "drupal/core-composer-scaffold": "^8.8 || ^9",
        "drush/drush": "^9.7.1 | ^10.0.0",
        "harvesthq/chosen": "^1.8",
        "oomphinc/composer-installers-extender": "^2.0",
        "sergeant/sgt_core": "~3.0",
        "sergeant/sgt_drupal_project_update": "~3.0",
        "sergeant/sgt_ignite": "~4.0",
        "sergeant/sgt_installation_profile": "~4.0",
        "vlucas/phpdotenv": "^4.0",
        "webflo/drupal-finder": "^1.0.0",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8 || ^9"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php",
            "scripts/sgt/sgt_drupal_project_update/SgtDrupalProjectUpdate/SgtScriptHandler.php",
            "scripts/sgt/sgt_drupal_project_update/SgtGitHub/SgtGitHub.php",
            "web/profiles/sgt/sgt_installation_profile/SgtInstallationProfile/SgtInstallationProfileScriptHandler.php",
            "web/themes/sgt_ignite/ignite_core/src/Composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "Drupal\\sgt_ignite\\Composer\\ScriptHandler::igniteUpdate"
        ],
        "update_sgt_drupal_project": "SgtDrupalProjectUpdate\\SgtScriptHandler::preUpdate"
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "enable-patching": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "scripts/sgt/sgt_drupal_project_update": [
                "sergeant/sgt_drupal_project_update"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "harvesthq/chosen"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/sgt/{$name}": [
                "sergeant/entity_creator_api",
                "sergeant/sgt_checklist",
                "sergeant/sgt_core",
                "sergeant/sgt_devel",
                "sergeant/sgt_field_paragraph_settings",
                "sergeant/sgt_filter_query_api",
                "sergeant/sgt_handbook",
                "sergeant/sgt_ignite_features",
                "sergeant/sgt_media_colorbox_field_formatter",
                "sergeant/sgt_media_crop",
                "sergeant/sgt_paragraphs",
                "sergeant/sgt_tiles"
            ],
            "web/themes/sgt_ignite/ignite_core": [
                "sergeant/sgt_ignite"
            ],
            "web/profiles/sgt/{$name}": [
                "sergeant/sgt_installation_profile"
            ]
        }
    }
}
