# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore SGT modules and theme directories generated by Composer
/web/modules/sgt/
/web/themes/sgt_ignite/ignite_core/
/web/profiles/

# Ignore sensitive information
# /web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore local services
/web/sites/*/local.services.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm (.idea) and Visual Studio Code (.vscode)
/.idea/
/.vscode/

# Ignore .env files as they are personal
/.env

# Ignore Drupal's tmp directory
/web/tmp/*
!/web/tmp/.gitkeep
