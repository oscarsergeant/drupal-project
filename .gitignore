######## Default git ignore rules under this line (Do not edit - use "Custom git ignore") ########

# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/

# Ignore SGT modules theme and script directories generated by Composer
/web/modules/sgt/
/web/themes/sgt_ignite/ignite_core/
/web/profiles/
/scripts/sgt/

# Ignore sensitive information
# /web/sites/*/settings.php
/web/sites/*/settings.local.php

# Ignore local and development services
/web/sites/*/local.services.yml
/web/sites/development.services.yml

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm (.idea) and Visual Studio Code (.vscode)
/.idea/
/.vscode/

# Ignore OS generated files
.DS_Store*
ehthumbs.db
Thumbs.db

# Ignore .env files as they are personal
/.env

# Ignore Drupal's tmp directory
/web/tmp/*
!/web/tmp/.gitkeep

# Ignore logs and databases
*.log
*.sql

# Ignore DDEV files
/.ddev/

# Ignore _local_backups
/_local_backups/

######## Custom git ignore rules under this line ########
